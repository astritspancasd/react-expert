    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.24",
    "axios": "^0.26.1",
    "formik": "^2.2.9",
    "local-storage": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
